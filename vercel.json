{
  "rewrites": [
    {
      "source": "/brand-identity",
      "destination": "/brand-identity.html"
    },
    {
      "source": "/landing-page",
      "destination": "/landing-page.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}

